    - define healAmount '<proc[dRegions_GetInheritedFlag].context[<def[world]>|<def[region]>|heal-amount]||null>'
    - if <def[healAmount].is[!=].to[null]||true> {
      - define healDelay '<proc[dRegions_GetInheritedFlag].context[<def[world]>|<def[region]>|heal-delay]||null>'
      - if <def[healDelay].is[==].to[null]||true> define healDelay '5'
      - define healMax '<proc[dRegions_GetInheritedFlag].context[<def[world]>|<def[region]>|heal-max]||null>'
      - if <def[healMax].is[==].to[null]||true> define healMax '20'
      - define healMax '<tern[<def[healMax].is[MORE].than[<player.health.max>]>]:<player.health.max>||<def[healMax]>>'
      - define healMin '<proc[dRegions_GetInheritedFlag].context[<def[world]>|<def[region]>|heal-min]||null>'
      - if <def[healMin].is[==].to[null]||true> define healMin '0'
      - if <queue.exists[dRegions_Heal_<player.name>]> queue q@dRegions_Heal_<player.name> clear
      - run locally healTask def:<def[region]>|<def[healAmount]>|<def[healDelay]>|<def[healMax]>|<def[healMin]> id:dRegions_Heal_<player.name>
      }

  healTask:
    - while <player.is_online> {
      - define world '<player.world.name.to_lowercase>'
      - define cuboids '<player.location.cuboids>'
      - inject instantly s@dRegions p:event_PreProc_OrderRegionsGlobal
      - if <def[orderedRegions].get[1].is[==].to[<def[1]>]> {
        - if <player.gamemode.id.is[==].to[1]> {
          - wait 1s
          - while next
          }
        - if <def[3].is[OR_LESS].than[0]> {
          - if <def[2].is[MORE].than[0]> {
            - if <player.health.is[LESS].than[<def[4]>]> {
              - adjust <player> 'health:<def[4]>'
              }
            }
            else {
            - if <player.health.is[MORE].than[<def[5]>]> {
              - animate <player> animation:hurt
              - adjust <player> 'health:<def[5]>'
              }
            }
          - wait 1s
          - while next
          }
        - if <def[2].is[MORE].than[0]> {
          - if <player.health.is[LESS].than[<def[4]>]> {
            - define newHealth '<player.health.add[<def[2]>]>'
            - adjust <player> 'health:<tern[<def[newHealth].is[MORE].than[<def[4]>]>]:<def[4]>||<def[newHealth]>>'
            }
          }
          else {
          - if <player.health.is[MORE].than[<def[5]>]> {
            - animate <player> animation:hurt
            - define newHealth '<player.health.add[<def[2]>]>'
            - adjust <player> 'health:<tern[<def[newHealth].is[LESS].than[<def[5]>]>]:<def[5]>||<def[newHealth]>>'
            }
          }
        - wait <def[3].as_duration>
        }
        else {
        - queue clear
        }
      }


